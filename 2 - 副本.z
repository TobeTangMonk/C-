#include<stdio.h>
#include<string.h>
#define N 1000
int letter,digit,space,other;
int main()

{
	void count(char s[]);
	char s[N];
	gets(s);
	count(s);
	printf("letter:%d\ndigit:%dspace:%d\nother:%d\n");
	return 0;
  	
	
}
void count(char s[])
{
	int i;
	letter=digit=space=other=0;
	
	for(i=0;s[i]!='\0';i++)
	{
		if(s[i]>='0'&&s[i]<='9')
      digit++;
      if(s[i]>='a'&&s[i]<='z'||s[i]>='A'&&s[i]<='Z')
      letter++;
      if(s[i]=' ')
       space++;
       else 
	  other++;
      
	 } 
}
